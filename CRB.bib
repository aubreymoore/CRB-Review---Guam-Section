	pages = {1017--24},
	file = {Crawford - 1989 - Engineering of an Oryctes baculovirus recombinant insertion of the polyhedrin gene from the Autographa californica nuc:/home/aubrey/Zotero/storage/9UHJ8KSU/Crawford - 1989 - Engineering of an Oryctes baculovirus recombinant insertion of the polyhedrin gene from the Autographa californica nuc.pdf:application/pdf}
