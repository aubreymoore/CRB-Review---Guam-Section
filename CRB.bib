@article{ramle_incidence_2005,
	title = {The incidence and use of {Oryctes} virus for control of rhinoceros beetle in oil palm plantations in {Malaysia}.},
	volume = {89},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/16039309},
	doi = {10.1016/j.jip.2005.02.009},
	abstract = {The rhinoceros beetle, Oryctes rhinoceros, has emerged as a serious pest of oil palm since the prohibition of burning as a method for maintaining estate hygiene in the 1990s. The abundance of beetles is surprising given that the Malay peninsula was the site of first discovery of the Oryctes virus, which has been used to effect good as a biological control agent in other regions. A survey of adult beetles was carried out throughout Malaysia using pheromone traps. Captured beetles were examined for presence of virus using both visual/microscopic examination and PCR detection methods. The survey indicated that Oryctes virus was common in Malaysia among the adult beetles. Viral DNA analysis was carried out after restriction with HindIII enzyme and indicated at least three distinct viral genotypes. Bioassays were used to compare the viral strains and demonstrate that one strain (type B) is the most virulent against both larvae and adults of the beetle. Virus type B has been cultured and released into healthy populations where another strain (type A) forms the natural background. Capture and examination of beetles from the release site and surrounding area has shown that the spread and persistence of the applied virus strain is accompanied by a reduction in palm frond damage.},
	number = {1},
	journal = {Journal of invertebrate pathology},
	author = {Ramle, M and Wahid, M B and Norman, K and Glare, T R and Jackson, Trevor A},
	month = may,
	year = {2005},
	keywords = {animals, Beetles, Beetles: virology, Baculoviridae, Baculoviridae: classification, Baculoviridae: genetics, Baculoviridae: pathogenicity, Biological, coco, DNA, Incidence, Malaysia, pest control, Plant Oils, Trees, Trees: parasitology, Viral, Virus Diseases, Virus Diseases: epidemiology},
	pages = {85--90},
	file = {Ramle - 2005 - Incidence and use of Oryctes virus for control of rhinoceros beetle in oil palm plantations in Malaysia:/home/aubrey/Zotero/storage/MIRHNJEK/Ramle - 2005 - Incidence and use of Oryctes virus for control of rhinoceros beetle in oil palm plantations in Malaysia.pdf:application/pdf;Ramle et al. - 2005 - The incidence and use of Oryctes virus for control of rhinoceros beetle in oil palm plantations in Malaysia:/home/aubrey/Zotero/storage/TTICRAUA/Ramle et al. - 2005 - The incidence and use of Oryctes virus for control of rhinoceros beetle in oil palm plantations in Malaysia.pdf:application/pdf}
}

@article{wertheim_pheromone-mediated_2005,
	title = {Pheromone-mediated aggregation in nonsocial arthropods: an evolutionary ecological perspective.},
	volume = {50},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/15355243},
	abstract = {Although the use of aggregation pheromones has been reported for hundreds of nonsocial arthropod species, the evolutionary ecological aspects of this behavior have received little attention. Despite the elaborate literature on mechanisms, robust data on costs and benefits of aggregation pheromones are scant. Existing literature indicates that, in contrast to the diversity of mechanisms, the ecological conditions in which aggregation pheromones are used are more alike. This points to a few general categories for costs and benefits of aggregation pheromones, and these are discussed. We subsequently review interspecific interactions that may be affected by the use of aggregation pheromones. We encounter a strikingly frequent association of aggregation pheromones with fungi and microorganisms and address cross-attraction by competitor species and exploitation by natural enemies. We show that aggregative behavior by individuals through the use of pheromones can profoundly affect ecological interactions and advocate further evolutionary and ecological investigations of pheromone-mediated aggregation.},
	number = {94},
	journal = {Annual Review of Entomology},
	author = {Wertheim, Bregje and Van Baalen, Erik-Jan A and Dicke, Marcel and Vet, Louise E M},
	year = {2005},
	keywords = {animals, Pheromones, Species Specificity, arthropods, arthropods physiology, environment, evolution, pheromones physiology, reproduction, social behavior},
	pages = {321--346}
}

